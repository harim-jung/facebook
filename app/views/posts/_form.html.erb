<%= form_for @post, html: { multipart: true } do |f| %>

  <%= f.text_area :content, placeholder: "What's on your mind?", cols: 64, rows: 8 %><br />


  <%= f.label "Upload images: "%>
  <%= f.file_field :image %><br /><br />

  <%= f.hidden_field :board_id, value: @board.id%>
  
  <%= f.submit %>

<% end %>